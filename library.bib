pages = {697--701},
publisher = {Anaesthesia Society of Anaesthetists},
title = {{Night-shift discharge from intensive care unit increases the mortality-risk of ICU survivors}},
url = {http://cat.inist.fr/?aModele=afficheN\&amp;cpsidt=16196476},
